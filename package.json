  "scripts": {
    "dev": "vite",
    "dev:backend": "dotenv -e .env.development -- cross-env NODE_ENV=development tsx watch src/backend/server/index.ts",
    "dev:all": "concurrently --kill-others \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "build:backend": "cross-env NODE_ENV=production tsc -p tsconfig.server.json",
    "build:all": "npm run build:backend && npm run build",
    "start:backend": "cross-env NODE_ENV=production node dist/backend/server/index.js",
    "start:prod": "cross-env NODE_ENV=production pm2 start ecosystem.config.cjs",
    "stop:prod": "pm2 stop ecosystem.config.cjs",
    "restart:prod": "pm2 restart ecosystem.config.cjs",
    "start": "concurrently --kill-others \"npm run dev:backend\" \"npm run dev\"",
    "serve": "vite preview",
    "generate-data": "node src/data/adapter.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "lint:duplicates": "jscpd src/ --ignore \"**/*.test.ts,**/*.spec.ts\"",
    "lint:full": "npm run lint && npm run lint:duplicates",
    "deps:analyze": "npx madge --ts-config tsconfig.json src/main.tsx",
    "deps:graph": "npx madge --ts-config tsconfig.json --image graph.png src/main.tsx",
    "deps:circular": "npx madge --ts-config tsconfig.json --circular src/main.tsx",
    "deps:list": "npx madge --ts-config tsconfig.json --json src/main.tsx > deps.json",
    "type-check": "tsc --noEmit",
    "lint:cache": "eslint src/utils/cache/**/*.ts --fix",
    "check-all": "npm run type-check && npm run lint:cache",
    "validate": "npm run type-check && npm run lint && npm run test",
    "prepare": "husky",
    "find:duplicates": "jscpd src/ --ignore \"**/*.test.ts,**/*.spec.ts\"",
    "find:duplicates:html": "jscpd src/ --reporter html --output jscpd-report",
    "find:duplicates:json": "jscpd src/ --reporter json --output jscpd-report.json",
    "quality:check": "npm run type-check && npm run lint:check && npm run find:duplicates",
    "quality:full": "npm run quality:check && npm run deps:circular",
    "check:unused-exports": "ts-unused-exports tsconfig.json",
    "analyze": "tsx scripts/rss-analyzer/rssStructureAnalyzer.ts",
    "electron": "electron electron/main.cjs",
    "build:wasm": "cd src/backend/license/wasm-validator && wasm-pack build --target web --out-dir pkg",
    "copy:wasm": "node -e \"require('fs-extra').copySync('./src/backend/license/wasm-validator/pkg', './src/premium/wasm/pkg', { overwrite: true }); console.log('âœ… WASM package copied to client source.');\"",
    "wasm": "npm run build:wasm && npm run copy:wasm",
    "generate:keys": "ts-node -P tsconfig.server.json src/backend/license/script/generate-keys.ts",
    "electron:debug": "cross-env ELECTRON_ENABLE_LOGGING=1 ELECTRON_ENABLE_STACK_DUMPING=1 electron --inspect=9229 --remote-debugging-port=9223 .",
    "electron:dev": "cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=1 ELECTRON_ENABLE_STACK_DUMPING=1 electron --inspect=9229 --remote-debugging-port=9223 ."
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
